 <!-- SENSEI ConfigurableAnalysis Configuration file.
      set enabled="1" on analyses you wish to enable -->
<sensei>

  <!--  VTK posthoc IO -->
  <analysis type="PosthocIO" output_dir="./paraview" file_name="output" mode="paraview" enabled="1">
    <mesh name="mesh">
      <cell_arrays>vtkGhostType, density, energy, pressure, viscosity, velocity</cell_arrays>
    </mesh>
  </analysis>

  <!--  VTK posthoc IO -->
  <analysis type="PosthocIO" output_dir="./visit" file_name="output" mode="visit" enabled="0">
    <mesh name="mesh">
      <cell_arrays>vtkGhostType, density, energy, pressure, viscosity, velocity</cell_arrays>
    </mesh>
  </analysis>

</sensei>
